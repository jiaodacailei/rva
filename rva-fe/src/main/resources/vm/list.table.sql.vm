CREATE TABLE `${table}` (
  `id` int NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `title` varchar(100) NOT NULL COMMENT '标题',
  `excerpt` varchar(200) COMMENT '摘要',
  `content` varchar(2000) COMMENT '内容',
  `image` varchar(1000) COMMENT '图片',
  `mode` varchar(20) COMMENT '排版模式',
  `disabled` varchar(1) default 'N' COMMENT '禁用',
  `category` varchar(300) COMMENT '分类',
  `group` varchar(300) COMMENT '分组',
  `uni_app_id` varchar(100) COMMENT '移动应用, rva_fe_app.id',
  `platform` varchar(100) COMMENT '平台，pc/ios/android等',
  `device` varchar(100) COMMENT '设备',
  `dept_id` bigint(20) COMMENT '部门',
  `tenant_id` varchar(100) COMMENT '租户',
  `create_by` bigint(20) COMMENT '创建者',
  `create_time` datetime DEFAULT NULL COMMENT '创建时间',
  `update_by` bigint(20) COMMENT '更新者',
  `update_time` datetime DEFAULT NULL COMMENT '更新时间',
  `data` text COMMENT '其他数据',
  PRIMARY KEY (`id`)
) COMMENT='${name}';